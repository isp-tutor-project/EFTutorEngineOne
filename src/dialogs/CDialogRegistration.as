s		public var Ssubmit:Button								public var Sreset:Button;								//************ Stage Symbols			/**		 * 		 */		public function CDialogRegistration() : void 		{									onReset(null);					}									override public function Destructor() : void		{			Ssubmit.removeEventListener(MouseEvent.CLICK, onSubmit);			Sreset.removeEventListener(MouseEvent.CLICK, onReset);							super.Destructor();		}				/**		 * 		 * @param	evt		 */		public function onSubmit(evt:MouseEvent) : void 		{								 	trace("onSubmit: ");					}							/**		 * 		 * @param	evt		 */		public function onReset(evt:MouseEvent) : void 		{								 	trace("onReset: ");		}				//****** Overridable Behaviors		/**		 * 		 * @param	Alpha		 */		override public function doModal(accounts:XMLList = null, Alpha:Number = 1, fAdd:Boolean = true) : void 		{					super.doModal(accounts, Alpha, fAdd);							Ssubmit.addEventListener(MouseEvent.CLICK, onSubmit);			Sreset.addEventListener(MouseEvent.CLICK, onReset);			}									/**		 * 		 * @param	evt		 */		override public function endModal(Result:String) : void 		{									Ssubmit.removeEventListener(MouseEvent.CLICK, onSubmit);			Sreset.removeEventListener(MouseEvent.CLICK, onReset);							super.endModal(Result);				}				//****** Overridable Behaviors					}}